{
  "name": "slice2md",
  "version": "0.1.0",
  "bin": {
    "slice2md": "lib/cli.js"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "build": "tsc -P tsconfig.build.json",
    "prepare": "npm test && npm run clean && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": "aikoven/slice2md",
  "author": "Daniel Lytkin <dan.lytkin@gmail.com>",
  "license": "GPL-2.0",
  "dependencies": {
    "assert-never": "^1.2.0",
    "dedent": "^0.7.0",
    "glob": "^7.1.4",
    "indent-string": "^4.0.0",
    "lodash": "^4.17.15",
    "markdown-table": "^1.1.3",
    "mkdirp": "^0.5.1",
    "slice2json": "^0.2.0"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.6.8",
    "@types/rimraf": "^2.0.2",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
